{
  "name": "cerebral-realworld-example-app",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yacoma/cerebral-realworld-example-app.git"
  },
  "keywords": [
    "cerebral"
  ],
  "author": "Henri Hulski <henri.hulski@gazeta.pl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yacoma/cerebral-realworld-example-app/issues"
  },
  "homepage": "https://conduit.yacoma.it",
  "scripts": {
    "build": "parcel build -d build public/index.html",
    "format": "npm run lint -- --fix && prettier --write \"*.md\" \"*.json\"",
    "lint": "eslint \"src/**/*.js\"",
    "start": "parcel serve -d build public/index.html --open",
    "test": "jest"
  },
  "dependencies": {
    "@cerebral/http": "^4.2.1",
    "@cerebral/react": "^3.2.1",
    "@cerebral/router": "^3.2.1",
    "@cerebral/storage": "^4.2.1",
    "cerebral": "^4.2.1",
    "jwt-decode": "^2.2.0",
    "marksy": "^6.0.3",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-jsx": "^5.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^22.4.3",
    "jest-localstorage-mock": "^2.2.0",
    "parcel-bundler": "^1.7.0",
    "prettier": "1.11.1",
    "react-test-renderer": "^16.3.1",
    "xhr-mock": "^2.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/app/**/*.js"
    ],
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/*.test.js"
    ],
    "testURL": "http://localhost"
  }
}
